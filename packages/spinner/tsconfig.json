{
  "extends": "../../tsconfig.json",
  "typedocOptions": {
    "entryPoints": [
      "./index.ts"
    ],
    "excludePrivate": true,
    "githubPages": false,
    "hideBreadcrumbs": true,
    "hideInPageTOC": true,
    "plugin": "typedoc-plugin-markdown",
    "publicPath": "https://github.com/daniellacosse/idea-spinner/tree/main/packages/spinner/docs/",
    "readme": "none"
  },
  "exclude": [
    "coverage"
  ]
}