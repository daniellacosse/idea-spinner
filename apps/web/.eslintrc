{
  "extends": [
    "../../.eslintrc",
    "plugin:vue/vue3-strongly-recommended"
  ],
  "root": true,
  "env": {
    "browser": true
  },
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "extraFileExtensions": [
      ".vue"
    ],
    "parser": "@typescript-eslint/parser",
    "project": [
      "./tsconfig.json"
    ],
    "sourceType": "module"
  },
  "plugins": [
    "vue"
  ],
  "rules": {
    "vue/multi-word-component-names": "off",
    "vue/singleline-html-element-content-newline": "off",
    "vue/html-self-closing": "error"
  }
}
